<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PIGui.Profil_chaufController">
   <children>
      <ImageView fitHeight="415.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Profil.png" />
         </image>
      </ImageView>
      <Label fx:id="lbUsername" layoutX="158.0" layoutY="47.0" prefHeight="27.0" prefWidth="118.0" textFill="#70dbc8">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="search" layoutX="512.0" layoutY="127.0" mnemonicParsing="false" onAction="#search" opacity="0.0" prefHeight="27.0" prefWidth="48.0" text="Search">
         <font>
            <Font name="Bell MT" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnDelete" layoutX="515.0" layoutY="186.0" mnemonicParsing="false" onAction="#delete" prefHeight="27.0" prefWidth="42.0" text="Delete account">
         <font>
            <Font name="Bell MT" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnEdit" layoutX="515.0" layoutY="286.0" mnemonicParsing="false" onAction="#edit" opacity="0.0" prefHeight="27.0" prefWidth="42.0" text="Edit">
         <font>
            <Font name="Bell MT" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnSignOut" layoutX="512.0" layoutY="323.0" mnemonicParsing="false" onAction="#signout" opacity="0.0" prefHeight="27.0" prefWidth="48.0" text="Sign out">
         <font>
            <Font name="Bell MT" size="14.0" />
         </font>
      </Button>
      <Label fx:id="lbMat" layoutX="290.0" layoutY="47.0" prefHeight="27.0" prefWidth="118.0" textFill="#70dbc8">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="170.0" layoutY="23.0" prefHeight="18.0" prefWidth="63.0" text="Nom:" />
      <Label layoutX="300.0" layoutY="23.0" prefHeight="18.0" prefWidth="63.0" text="Matricule" />
